<p id="notice"><%= notice %></p>

<h1>Listing Cc Questions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cc_questions.each do |cc_question| %>
      <tr>
        <td><%= cc_question.question %></td>
        <td><%= link_to 'Show', cc_question %></td>
        <td><%= link_to 'Edit', edit_cc_question_path(cc_question) %></td>
        <td><%= link_to 'Destroy', cc_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cc question', new_cc_question_path %>
